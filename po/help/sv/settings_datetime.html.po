#. type: Content of: <h3>
#: admin/views/default/help/en/settings_datetime.html:2
msgid "Date and time"
msgstr "Datum och tid"

#. type: Content of: <p>
#: admin/views/default/help/en/settings_datetime.html:5
msgid "<strong>Current timezone is</strong> - Shows the selected timezon."
msgstr "<strong>Nuvarande tidszon är</strong> - Visar nuvarande tidszon"

#. type: Content of: <p>
#: admin/views/default/help/en/settings_datetime.html:8
msgid "<strong>Select timezone</strong> - Select your timezone in the drop down list."
msgstr "<strong>Välj tidszon</strong> - Välj din tidszon i listan. "

#. type: Content of: <p>
#: admin/views/default/help/en/settings_datetime.html:11
msgid "<strong>Set time automatically</strong> - This enables {PLATFORM} to set the date and time automatically via an internet server."
msgstr "<strong>Ställ in tid automatiskt</strong> - Detta låter {PLATFORM} ställa in tiden automatiskt från en tidsserver på internet. "

#. type: Content of: <p>
#: admin/views/default/help/en/settings_datetime.html:14
msgid "<strong>Date</strong> - Set the system date manually."
msgstr "<strong>Datum</strong> - Ställ in {PLATFORM}'s datum automatiskt."

#. type: Content of: <p>
#: admin/views/default/help/en/settings_datetime.html:17
msgid "<strong>Time</strong> - Set the system time manually."
msgstr "<strong>Tid</strong> - Ställ in {PLATFORM}'s tid manuellt."

#. type: Content of: <h3>
#: admin/views/default/help/en/settings_datetime.html:20
msgid "Default system language"
msgstr "Standardspråk"

#. type: Content of: <p>
#: admin/views/default/help/en/settings_datetime.html:23
msgid "<strong>System language</strong> - Select language for the user interface."
msgstr "<strong>Systemspråk</strong> - Välj språk för användarinterfacet. "
